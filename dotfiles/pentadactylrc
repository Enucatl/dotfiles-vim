set complete=sfbS
" use a different color scheme

colorscheme solarized-light

" Focus first input.. fast
map i gi

" Open help, prefs, addons, downloads in new tab
set newtab=all

" Pattern for next/prev links
set nextpattern=\\bnastępn,\\bdalej,\\bnext,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b,\\bsucc,\\bavanti
set previouspattern=\\bpoprzedni,\\bwstecz,\\bprev|previous\\b,^<$,^(<<|«)$,^(<|«),(<|«)$,\\bprec

nmap -builtin ; :

".pentadactylrc
map -modes=n,v C :firebug<Space>disable<Space><Return>
map -modes=n,v ;kk :firebug<Space>console<Return>
map -modes=n,v ;kr :firebug<Space>run<Return>
map -modes=n,v ;kc :firebug<Space>clear<Return>

"run script on blur firebug console
set fbliverun
