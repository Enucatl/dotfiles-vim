alias l='ls -lkh'
alias la='ls -la'
alias du='du -lkh --max-depth 1'
alias df='df -kTh'
alias mkdir='mkdir -p'
alias rm='rm -I'
alias mv='mv -i'
alias cp='cp -i'
alias root='root -l'
alias py='jupyter console'
alias c='clear'
alias cclear='cd && clear'
alias convert300='convert -density 300'
alias R='R --no-save --no-restore-data --quiet'

# setup nvim-qt with lunarvim
alias vi='nvim-qt --nvim ~/.local/bin/lvim'
