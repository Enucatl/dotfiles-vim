alias l='ls -lkh'
alias la='ls -la'
alias du='du -lkh --max-depth 1'
alias df='df -kTh'
alias rm='rm -I'
alias mv='mv -i'
alias mkdir='mkdir -p'
alias cp='cp -i'
alias root='root -l'
alias ssh='ssh -Y'
alias python='python -t -W ignore::DeprecationWarning'
alias ipython='ipython --colors LightBG'
alias py='ipython'
alias c='clear'
alias cclear='cd && clear'
alias ag='sudo apt-get'
alias ac='apt-cache'
alias convert300='convert -density 300'
alias R='R --no-save --no-restore-data --quiet'
